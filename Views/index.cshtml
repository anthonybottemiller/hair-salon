@{
  Layout="Shared/_Layout.cshtml";
}
<div class="container">
  <h1>Sandy Hair Salon</h1>
  <div class="col-sm-6">
    @if (Model.Count == 0)
    {
      <p>No stylists are in the system yet!</p>
    }
    else
    {
      <ul>
      @foreach (var stylist in Model)
      {
        <li><a href="/stylists/@stylist.GetId()"</a> @stylist.GetName()</li>
      }
      </ul>
    }
    <a href="/stylists/new">New Stylist</a>
    <a href="/clients/new">New Client</a>
  </div>
</div>
